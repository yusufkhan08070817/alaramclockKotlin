<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <gradient
                android:angle="90"
                android:centerColor="#1408375C"
                android:centerY="0.5"
                android:endColor="@color/white"
                android:startColor="#9E000000" />
        </shape>
    </item>
    <item
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp">
        <shape android:shape="oval">
            <solid android:color="@color/white" />
        </shape>
    </item>
    <item
        android:width="100dp"
        android:height="100dp">
        <layer-list>
            <item
                android:width="1dp"
                android:height="3dp"
                android:gravity="center_horizontal"
                android:top="4dp">
                <shape android:shape="rectangle">
                    <solid android:color="@color/black" />
                </shape>
            </item>
            <item
                android:width="1dp"
                android:height="3dp"

                android:gravity="center_horizontal"
                android:top="94dp">
                <shape android:shape="rectangle">
                    <solid android:color="@color/black" />
                </shape>
            </item>
            <item
                android:width="1dp"
                android:height="3dp"
                android:gravity="center_vertical"
                android:left="4dp">
                <rotate android:fromDegrees="90">
                    <shape android:shape="rectangle">

                        <solid android:color="@color/black" />
                    </shape>
                </rotate>

            </item>
            <item
                android:width="1dp"
                android:height="3dp"
                android:gravity="center_vertical"
                android:left="27dp"

                android:top="79dp">
                <rotate android:fromDegrees="30">
                    <shape android:shape="rectangle">

                        <solid android:color="@color/black" />
                    </shape>
                </rotate>

            </item>

            <item
                android:width="1dp"
                android:height="3dp"
                android:gravity="center_horizontal"
                android:left="47dp"
                android:top="9dp">
                <rotate android:fromDegrees="30">
                    <shape android:shape="rectangle">

                        <solid android:color="@color/black" />
                    </shape>
                </rotate>

            </item>


            <item
                android:width="1dp"
                android:height="3dp"
                android:gravity="center_vertical"
                android:left="10dp"
                android:top="46dp">
                <rotate android:fromDegrees="60">
                    <shape android:shape="rectangle">

                        <solid android:color="@color/black" />
                    </shape>
                </rotate>

            </item>
            <item
                android:width="1dp"
                android:height="3dp"
                android:gravity="center_horizontal"

                android:left="79dp"
                android:top="26dp">
                <rotate android:fromDegrees="60">
                    <shape android:shape="rectangle">

                        <solid android:color="@color/black" />
                    </shape>
                </rotate>

            </item>

            <item
                android:width="1dp"
                android:top="26dp"
                android:right="78dp"
                android:height="3dp"
                android:gravity="center_horizontal">
                <rotate android:fromDegrees="120">
                    <shape android:shape="rectangle">

                        <solid android:color="@color/black" />
                    </shape>
                </rotate>

            </item>
            <item
                android:width="1dp"
                android:top="46dp"
                android:left="89dp"
                android:height="3dp"
                android:gravity="center_vertical">
                <rotate android:fromDegrees="120">
                    <shape android:shape="rectangle">

                        <solid android:color="@color/black" />
                    </shape>
                </rotate>

            </item>

            <item
                android:width="1dp"
                android:height="3dp"
                android:right="46dp"
                android:top="9dp"
                android:gravity="center_horizontal">
                <rotate android:fromDegrees="150">
                    <shape android:shape="rectangle">

                        <solid android:color="@color/black" />
                    </shape>
                </rotate>

            </item>
            <item
                android:width="1dp"
                android:height="3dp"
                android:left="72dp"
                android:top="79dp"
                android:gravity="center_vertical">
                <rotate android:fromDegrees="150">
                    <shape android:shape="rectangle">

                        <solid android:color="@color/black" />
                    </shape>
                </rotate>

            </item>
            <item
                android:width="1dp"
                android:height="3dp"
                android:gravity="center_vertical"
                android:left="95dp">
                <rotate android:fromDegrees="90">
                    <shape android:shape="rectangle">

                        <solid android:color="@color/black" />
                    </shape>
                </rotate>

            </item>
            <item
                android:width="3dp"
                android:height="3dp"
                android:gravity="center"
                >
                <rotate android:fromDegrees="90">
                    <shape android:shape="oval">

                        <solid android:color="@color/black" />
                    </shape>
                </rotate>

            </item>


        </layer-list>

    </item>
</layer-list>